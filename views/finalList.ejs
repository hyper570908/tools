<% if (test === 1) { %>
	<%- include extend/header %>
<% } if (test === 2) { %>
	<%- include extend/myHeader %>
<% } else if (test === 3) { %>
	<%- include extend/sb_header %>
<% } %>
	<% if(finalList === null){ %>
		這是首頁,找不到資料
	<% }else{ %>

		<% var keys = Object.keys(finalList); %>

		<table id="table1"  class="display" cellspacing="0" width="100%">
		<thead>
			<tr style="color:#428bca">
			<th>Item</th>
			<th>MAC</th>
			<th>Last Repor</th>
			<th>溫度</th>
			<th>濕度</th>
			<th>Status</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0;i<keys.length ;i++){ %>
				<tr>
					<td><%= i+1%></td>
					<td><%= finalList[keys[i]].macAddr%></td>
					<td><%= finalList[keys[i]].date%></td>
					<td><%= finalList[keys[i]].information.temperature%></td>
					<td><%= finalList[keys[i]].information.humidity%></td>
					<td>
					<% if(finalList[keys[i]].overtimr){ %>
						<img src="/icons/connection_fail.png" width="ˇ30" height="30" name="status">
					<% }else{ %>
						<img src="/icons/connection_ok.png" width="30" height="30" name="status">
					<% } %>
					</td>
				</tr>
			<% } %>

		</tbody>
	</table>

	<% } %>

<% if(test === 1){ %>
	<%- include extend/footer %>
<% } if (test === 2) { %>
	<%- include extend/myFooter %>
<% } else if (test === 3){ %>
	<%- include extend/sb_footer %>
<% } %>