<% if (test === 1) { %>
	<%- include extend/header %>
<% } if (test === 2) { %>
	<%- include extend/myHeader %>
<% } else if (test === 3) { %>
	<%- include extend/sb_header %>
<% } %>
<form method="post">

  mac：  <input type="text" name="mac"/><br />
           <input type="submit" value="查詢"/>
</form>


<% if (devices) { %>
    <table width=“500" border="1">
	<tr>
		<td>mac位址</td>
		<td>資料</td>
		<td>接收時間</td>
	</tr>
	<% devices.forEach(function(device) { %>
		<tr>
		<td><%= device.macAddr %></td>
		<td><%= device.data %></td>
		<td><%= device.date %></td>
		</tr>
	<% }); %>
	</table>
<% } %>

<% if(test === 1){ %>
	<%- include extend/footer %>
<% } if (test === 2) { %>
	<%- include extend/myFooter %>
<% } else if (test === 3){ %>
	<%- include extend/sb_footer %>
<% } %>